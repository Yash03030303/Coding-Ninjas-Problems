#include <iostream>
#include <string>
#include <vector>
#include <sstream>
using namespace std;

bool isVowel(char c) {
    c = tolower(c);
    return (c == 'a' || c == 'e' || c == 'i' || c == 'o' || c == 'u');
}

int main() {
    int t;
    cin >> t;
    cin.ignore(); // To ignore the newline character after reading

    vector<string> sentences(t);
    for (int i = 0; i < t; ++i) {
        getline(cin, sentences[i]);
    }

    for (int i = 0; i < t; ++i) {
        string s = sentences[i];
        int words = 0, vowels = 0, consonants = 0;

        // Count words
        istringstream iss(s);
        string word;
        while (iss >> word) {
            ++words;
        }

        for (int j = 0; j < s.length(); ++j) {
            char c = s[j];
            if (isalpha(c)) {
                if (isVowel(c)) {
                    ++vowels;
                } else {
                    ++consonants;
                }
            }
        }

        cout << words << " " << vowels << " " << consonants << endl;
    }

    return 0;
}
